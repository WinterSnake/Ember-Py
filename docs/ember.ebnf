program: statements*;
statement: conditional | (expression ';');
conditional: if '(' expression ')' '{' statement* '}' ('else' '{' statement* '}')?;
expression: expression_binary;
expression_binary: primary (OPERATOR primary)*;
primary: IDENTIFIER | NUMBER | '(' expression ')';

OPERATOR: '+' | '-' | '*' | '/' | '%' | '<' | '>' | '<=' | '>=' | '==';
